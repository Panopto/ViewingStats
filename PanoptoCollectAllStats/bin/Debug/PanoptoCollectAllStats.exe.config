<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IAccessManagement">
                    <security mode="Transport" />
                </binding>
                <binding name="BasicHttpBinding_IUserManagement">
                    <security mode="Transport" />
                </binding>
                <binding name="BasicHttpBinding_ISessionManagement">
                    <security mode="Transport" />
                </binding>
                <binding name="BasicHttpBinding_IUsageReporting">
                    <security mode="Transport" />
                </binding>
                <binding name="BasicHttpBinding_ISessionManagement1">
                    <security mode="Transport" />
                </binding>
                <binding name="BasicHttpBinding_IUserManagement1">
                    <security mode="Transport" />
                </binding>
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="https://chk.hosted.panopto.com/Panopto/PublicAPI/4.6/AccessManagement.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IAccessManagement"
                contract="PanoptoAccessManagement.IAccessManagement" name="BasicHttpBinding_IAccessManagement" />
            <endpoint address="https://chk.hosted.panopto.com/Panopto/PublicAPI/4.6/UserManagement.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IUserManagement"
                contract="PanoptoUserManagement.IUserManagement" name="BasicHttpBinding_IUserManagement" />
            <endpoint address="https://chk.hosted.panopto.com/Panopto/PublicAPI/4.6/SessionManagement.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ISessionManagement"
                contract="PanoptoSessionManagement.ISessionManagement" name="BasicHttpBinding_ISessionManagement" />
            <endpoint address="https://chk.hosted.panopto.com/Panopto/PublicAPISSL/4.6/UsageReporting.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IUsageReporting"
                contract="UsageReporting.IUsageReporting" name="BasicHttpBinding_IUsageReporting" />
            <endpoint address="https://chk.hosted.panopto.com/Panopto/PublicAPISSL/4.6/SessionManagement.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ISessionManagement1"
                contract="SessionManagement.ISessionManagement" name="BasicHttpBinding_ISessionManagement1" />
            <endpoint address="https://chk.hosted.panopto.com/Panopto/PublicAPISSL/4.6/UserManagement.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IUserManagement1"
                contract="UserManagement.IUserManagement" name="BasicHttpBinding_IUserManagement1" />
        </client>
    </system.serviceModel>
</configuration>